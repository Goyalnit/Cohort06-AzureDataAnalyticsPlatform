{
	"name": "df_cases_deaths_transform",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_df_cases_deaths",
						"type": "DatasetReference"
					},
					"name": "CasesAndDeathsSource"
				},
				{
					"name": "CountryLookup"
				}
			],
			"sinks": [
				{
					"name": "casesdeathoutput"
				}
			],
			"transformations": [
				{
					"name": "filterOnlyEurpore",
					"description": "Filter is applied to have only continent as Europe and non-null valued in country code."
				},
				{
					"name": "SelectfewColumns"
				},
				{
					"name": "PivotCasesAndDeathValues"
				},
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as integer,",
				"          date as date,",
				"          rate_14_day as string,",
				"          source as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CasesAndDeathsSource",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> CountryLookup",
				"CasesAndDeathsSource filter(continent == 'Europe' && not(isNull(country_code))) ~> filterOnlyEurpore",
				"filterOnlyEurpore select(mapColumn(",
				"          country,",
				"          country_code,",
				"          population,",
				"          indicator,",
				"          daily_count,",
				"          source,",
				"          each(match(name == 'date'),",
				"               'reported' + '_date' = $$)",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> SelectfewColumns",
				"SelectfewColumns pivot(groupBy(country,",
				"          country_code,",
				"          population,",
				"          reported_date,",
				"          source),",
				"     pivotBy(indicator),",
				"     count = sum(daily_count),",
				"     columnNaming: '$V_$N',",
				"     lateral: true) ~> PivotCasesAndDeathValues",
				"PivotCasesAndDeathValues, CountryLookup lookup(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> casesdeathoutput"
			]
		}
	}
}