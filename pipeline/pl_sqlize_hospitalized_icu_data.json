{
	"name": "pl_sqlize_hospitalized_icu_data",
	"properties": {
		"activities": [
			{
				"name": "Copy ProcessedData to Ded SQL pool",
				"description": "Copy hospitalization & Icu data to Ded SQL Pool",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"preCopyScript": "TRUNCATE table covid_reporting.cases_and_deaths;",
						"writeBehavior": "Insert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "country",
									"type": "String"
								},
								"sink": {
									"name": "country",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "country_code_2_digit",
									"type": "String"
								},
								"sink": {
									"name": "country_code_2_digit",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "country_code_3_digit",
									"type": "String"
								},
								"sink": {
									"name": "country_code_3_digit",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "population",
									"type": "String"
								},
								"sink": {
									"name": "population",
									"type": "Int64"
								}
							},
							{
								"source": {
									"name": "confirmed_count",
									"type": "String"
								},
								"sink": {
									"name": "cases_count",
									"type": "Int64"
								}
							},
							{
								"source": {
									"name": "deaths_count",
									"type": "String"
								},
								"sink": {
									"name": "deaths_count",
									"type": "Int64"
								}
							},
							{
								"source": {
									"name": "reported_date",
									"type": "String"
								},
								"sink": {
									"name": "reported_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"name": "source",
									"type": "String"
								},
								"sink": {
									"name": "source",
									"type": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_proessed_hospital_admission_daily",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sqlpool_cases_deaths",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}