									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "week_end_date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "week_end_date",
									"type": "DateTime",
									"physicalType": "date"
								}
