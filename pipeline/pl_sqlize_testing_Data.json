{
	"name": "pl_sqlize_testing_Data",
	"properties": {
		"activities": [
			{
				"name": "Copy Processed Testing Data to Ded SQL pool",
				"description": "Copy Testing data to Ded SQL Pool",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
