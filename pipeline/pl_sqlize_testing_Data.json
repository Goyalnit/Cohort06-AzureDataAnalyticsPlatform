								},
								"sink": {
									"name": "country_code_3_digit",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "year_week",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "year_week",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "week_start_date",
									"type": "String",
									"physicalType": "String"
